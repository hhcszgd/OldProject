#import "SystemAttributes.h"


@implementation SystemAttributes

static CGFloat keyboardHeight = 0.0;


+ (void)setKeyboardHeight:(CGFloat)height {
	//NSLog(@"keyboard height is %f", height);
	keyboardHeight = height;
}


+ (int)getKeyboardHeight {
	return keyboardHeight;
}

@end
